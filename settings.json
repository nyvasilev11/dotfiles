{
    "workbench.startupEditor": "newUntitledFile",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "'PTMono', Consolas, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.minimap.showSlider": "always",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.snippetSuggestions": "top",
    "editor.suggest.insertHighlight": true,
    "editor.suggest.maxVisibleSuggestions": 10,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "workbench.tree.renderIndentGuides": "always",
    "window.menuBarVisibility": "compact",
    "window.zoomLevel": -0.5,
    "search.enableSearchEditorPreview": true,
    "search.showLineNumbers": true,
    "search.useGlobalIgnoreFiles": true,
    "terminal.integrated.macOptionIsMeta": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "css.lint.duplicateProperties": "warning",
    "css.lint.float": "warning",
    "css.lint.important": "warning",
    "css.lint.universalSelector": "warning",
    "emmet.triggerExpansionOnTab": true,
    "html.format.endWithNewline": true,
    "html.mirrorCursorOnMatchingTag": false,
    "scss.lint.duplicateProperties": "warning",
    "scss.lint.float": "warning",
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.colorTheme": "Cobalt2",
    "extensions.ignoreRecommendations": false,
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "editor.tokenColorCustomizations": {
        "[Cobalt2]": {
            "comments": "#ae7a2c"
        }
    },
    "workbench.colorCustomizations": {
        "editorIndentGuide.activeBackground": "#c7c234",
        "editorBracketMatch.background": "#415F73",
        "editor.lineHighlightBackground": "#2A4558",
        "editor.selectionBackground": "#415F73",
        "editor.selectionHighlightBackground": "#2A4558",
        "editor.selectionHighlightBorder": "#E4D9C5",
        "activityBar.foreground": "#E4D9C5",
        "terminal.ansiBrightBlack": "#a88",
        "editor.foldBackground": "#2c4250"
    },
    "explorer.openEditors.visible": 0,
    "editor.tabSize": 2,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.smoothScrolling": true,
    "editor.wordWrap": "on",
    "workbench.editor.enablePreview": false,
    "explorer.confirmDelete": false,
    "prettier.jsxBracketSameLine": true,
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.suggest.autoImports": false,
    "javascript.suggest.autoImports": false,
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.renderSideBySide": true,
    "workbench.list.keyboardNavigation": "filter",
    "timeline.showView": true,
    "editor.semanticHighlighting.enabled": true
}
