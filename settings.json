{
    "workbench.colorTheme": "Cobalt2",
    "editor.fontFamily": "'PTMono',' Droid Sans Mono', 'Droid Sans Fallback'",
    "editor.cursorStyle": "block",
    "editor.tabCompletion": "on",
    "editor.formatOnSave": true,
    "editor.suggest.maxVisibleSuggestions": 10,
    "files.trimFinalNewlines": true,
    "workbench.list.multiSelectModifier": "alt",
    "workbench.preferredDarkColorTheme": "Cobalt2",
    "workbench.editor.closeOnFileDelete": true,
    "window.zoomLevel": -0.5,
    "explorer.openEditors.visible": 0,
    "search.smartCase": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.tokenColorCustomizations": {
        "[Cobalt2]": {
            "comments": "#AE7A2C"
        }
    },
    "workbench.colorCustomizations": {
        "editorIndentGuide.activeBackground": "#C7C234",
        "editorBracketMatch.background": "#415F73",
        "editor.lineHighlightBackground": "#2A4558",
        "editor.selectionBackground": "#415F73",
        "editor.selectionHighlightBackground": "#2A4558",
        "activityBar.foreground": "#E4D9C5",
        "terminal.ansiBrightBlack": "#a88",
        "editor.foldBackground": "#2C4250",
    },
    "highlight-matching-tag.styles": {
        "opening": {
            "full": {
                "highlight": "rgba(255,198,0, 0.2)"
            }
        }
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "editor.renderWhitespace": "all",
    "editor.definitionLinkOpensInPeek": true,
    "editor.rulers": [
        {
            "column": 80,
            "color": "#2A4558"
        },
        {
            "column": 100,
            "color": "#2A4558"
        }
    ],
}
